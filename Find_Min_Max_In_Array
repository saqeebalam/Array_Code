package array;

public class Min_MaxElement_Character {
	public static void main(String[] args) {
		
		  char arr[]= {'a','v','f','h','z'}; 
		  char max=arr[0]; 
		  char min=arr[0]; 
		 
		
			for(int i=0;i<arr.length-1;i++) {
			if(max<arr[i+1]) {//logic for maximum element
				max=arr[i+1];
			}else if(min>arr[i+1]) {//logic for minimum element
				min=arr[i+1];
			}
			
		}
		
		System.out.println("Maximum element="+max);
		System.out.println("Minimum element="+min);
	}

}
