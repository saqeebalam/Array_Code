//WAP to print missing element present in an ARRAY from 1 to 10
package array;

public class FIndMissingElement {
	public static void main(String[] args) {
		int arr[]= {1,2,3,4,5,6,8,9,10};//sorted array required and put 1 extra number
		int j=1;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]!=j) {//missing element find 
				System.out.println("missing "+j);
				break;
			}
			j++;
		}
	}

}
